{
  "name": "@expo/next-adapter",
  "version": "4.0.12",
  "description": "Adapter for using Next.js with Expo modules",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "watch": "tsc --watch --preserveWatchOutput",
    "build": "tsc",
    "prepare": "yarn run clean && yarn build",
    "clean": "rimraf build ./tsconfig.tsbuildinfo",
    "lint": "eslint ."
  },
  "bin": {
    "next-expo": "./build/cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo-cli.git",
    "directory": "packages/next-adapter"
  },
  "keywords": [
    "expo",
    "expo-web",
    "json",
    "next",
    "react",
    "react-native",
    "react-dom",
    "react-native-web"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/expo-cli/issues"
  },
  "homepage": "https://github.com/expo/expo-cli/tree/main/packages/next-adapter#readme",
  "files": [
    "babel",
    "build"
  ],
  "peerDependencies": {
    "next": "^11",
    "react": "^17",
    "react-native-web": "^0.17.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@expo/webpack-config": "0.17.2",
    "resolve-from": "^5.0.0",
    "update-check": "^1.5.3"
  }
