    "clean": "rm -rf build ./tsconfig.tsbuildinfo",
    "watch": "concurrently \"tsc --emitDeclarationOnly --watch\" \"gulp watch\"",
    "test": "jest --config jest/unit-test-config.json --rootDir .",
