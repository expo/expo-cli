{
  "name": "@expo/config-plugins",
  "version": "3.0.3",
  "description": "A library for Expo config plugins",
  "main": "build/index.js",
  "scripts": {
    "watch": "tsc --watch --preserveWatchOutput",
    "build": "tsc --emitDeclarationOnly && babel src --out-dir build --extensions \".ts\" --source-maps --ignore \"src/**/__mocks__/*\",\"src/**/__tests__/*\",\"src/**/__integration_tests__/*\"",
    "prepare": "yarn run clean && yarn build",
    "clean": "rimraf build ./tsconfig.tsbuildinfo",
    "lint": "eslint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo-cli.git",
    "directory": "packages/config-plugins"
  },
  "keywords": [
    "json",
    "expo",
    "react-native",
    "react"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/expo-cli/issues"
  },
  "homepage": "https://docs.expo.io/guides/config-plugins/",
  "files": [
    "build",
    "paths"
  ],
  "dependencies": {
    "@expo/config-types": "^41.0.0",
    "@expo/json-file": "8.2.30",
    "@expo/plist": "0.0.13",
    "debug": "^4.3.1",
    "find-up": "~5.0.0",
    "fs-extra": "9.0.0",
    "getenv": "^1.0.0",
    "glob": "7.1.6",
    "resolve-from": "^5.0.0",
    "slash": "^3.0.0",
    "xcode": "^3.0.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/find-up": "^4.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/xml2js": "^0.4.5",
    "memfs": "^2.15.5",
    "rimraf": "^3.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
