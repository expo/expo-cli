{
  "name": "install-expo-modules",
  "version": "0.0.1",
  "description": "Tools to install expo-modules for existing react-native projects",
  "main": "build/index.js",
  "scripts": {
    "watch": "tsc --watch --preserveWatchOutput",
    "build": "tsc",
    "prepare": "yarn run clean && yarn build",
    "clean": "rimraf build ./tsconfig.tsbuildinfo",
    "lint": "eslint .",
    "test": "jest"
  },
  "bin": {
    "install-expo-modules": "./bin/migrate.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo-cli.git",
    "directory": "packages/install-expo-modules"
  },
  "keywords": [
    "expo",
    "react-native",
    "react"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/expo-cli/issues"
  },
  "homepage": "https://github.com/expo/expo-cli/tree/master/packages/install-expo-modules#readme",
  "files": [
    "build",
    "!*/__tests__/*"
  ],
  "dependencies": {
    "@expo/config": "6.0.2",
    "@expo/config-plugins": "4.0.2",
    "@expo/package-manager": "0.0.47",
    "chalk": "^4.1.2",
    "find-up": "^5.0.0",
    "fs-extra": "9.0.0",
    "glob": "7.1.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
