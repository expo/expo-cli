    "@types/invariant": "^2.2.30",
    "find-yarn-workspace-root": "^1.2.1",
    "fs-extra": "^7.0.1",
    "invariant": "^2.2.4",
    "js-yaml": "^3.13.1",
    "resolve-from": "^5.0.0",
    "slugify": "^1.3.4",
