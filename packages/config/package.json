{
  "name": "@expo/config",
  "version": "2.5.2",
  "description": "A library for interacting with the app.json",
  "main": "build/index.js",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "prepare": "yarn run clean && yarn build",
    "clean": "rm -rf build ./tsconfig.tsbuildinfo",
    "lint": "eslint .",
    "test": "jest"
  },
  "jest": {
