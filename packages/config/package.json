  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/expo-cli/issues"
  },
  "homepage": "https://github.com/expo/expo-cli/tree/master/packages/config#readme",
  "files": [
    "build"
  ],
  "dependencies": {
