{
  "name": "@expo/dev-tools",
  "version": "0.13.8",
  "main": "build/server/index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch server -e js,ts --exec \"ts-node ./server/dev-server\"",
    "build": "yarn clean && yarn build-server && yarn extract-fragment-types && yarn build-client",
    "build-client": "NODE_ENV=production GENERATE_SOURCEMAP=false ts-node ./build-client",
    "build-server": "NODE_ENV=production tsc",
    "extract-fragment-types": "ts-node ./server/extract-fragment-types",
    "clean": "rimraf ./build",
    "prepare": "yarn run clean && yarn run build",
    "start": "yarn dev",
    "test": "jest --config jest.config.js"
  },
  "files": [
    "build"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@expo/config": "3.2.3",
    "base64url": "3.0.1",
    "express": "4.16.4",
    "freeport-async": "2.0.0",
    "graphql": "0.13.2",
    "graphql-tools": "3.0.0",
    "iterall": "1.2.2",
    "lodash": "^4.17.15",
    "subscriptions-transport-ws": "0.9.8"
  },
  "peerDependencies": {
    "@expo/xdl": "*"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@expo/babel-preset-cli": "0.2.13",
    "@expo/xdl": "57.9.5",
    "anser": "^1.4.7",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link-http": "^1.5.4",
    "apollo-link-ws": "^1.0.8",
    "apollo-server-express": "^1.3.6",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "next": "^8.0.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "typescript": "3.7.3"
  },
  "license": "MIT",
