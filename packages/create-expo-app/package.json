{
  "name": "create-expo-app",
  "version": "0.0.1",
  "main": "build",
  "description": "Create universal Expo apps",
  "license": "BSD-3-Clause",
  "keywords": [
    "expo",
    "react-native",
    "react"
  ],
  "homepage": "https://docs.expo.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo-cli.git",
    "directory": "packages/create-expo-app"
  },
  "author": "Evan Bacon <bacon@expo.io> (https://github.com/evanbacon)",
  "bin": {
    "create-expo-app": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "prepublishOnly": "yarn run clean && yarn run build:prod",
    "lint": "eslint .",
    "watch": "yarn run build -w",
    "build": "ncc build ./src/index.ts -o build/",
    "build:prod": "ncc build ./src/index.ts -o build/ --minify --no-cache --no-source-map-register",
    "clean": "rimraf ./build/"
  },
  "devDependencies": {
    "@expo/package-manager": "0.0.52",
    "@expo/spawn-async": "^1.5.0",
    "@types/cacache": "^15.0.1",
    "@types/form-data": "^2.2.0",
    "@types/fs-extra": "^8.1.0",
    "@types/getenv": "^1.0.0",
    "@types/node": "^12.6.8",
    "@types/prompts": "2.0.8",
    "@types/tar": "4.0.3",
    "cacache": "^15.3.0",
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "execa": "^4.0.1",
    "form-data": "^3.0.1",
    "getenv": "^1.0.0",
    "minipass": "^3.1.1",
    "node-fetch": "^2.6.0",
    "ora": "^4.0.3",
    "prompts": "2.3.2",
    "tar": "^6.0.1",
    "terminal-link": "^2.1.1",
    "update-check": "1.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
