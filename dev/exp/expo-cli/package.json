{
  "name": "expo-cli",
  "version": "1.0.0",
  "description": "The command-line tool for creating and publishing Expo apps",
  "preferGlobal": true,
  "main": "build/expo.js",
  "files": [
    "build",
    "bin"
  ],
  "scripts": {
    "start": "yarn run build && yarn run watch",
    "build": "gulp clean && gulp build",
    "watch": "gulp watch",
    "prepare": "yarn build",
    "pkg": "pkg ."
  },
  "bin": {
    "expo": "./bin/expo.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expo/exp.git"
  },
  "keywords": [
    "expo",
    "react-native"
  ],
  "author": "support@expo.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/expo/issues"
  },
  "homepage": "https://github.com/expo/exp#readme",
  "engines": {
    "node": ">=6.9.1"
  },
  "pkg": {
    "scripts": "build/**/*.js"
  },
  "powertools": {
    "group": [
      "dev-tools",
      "exp"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-inline-imports-commonjs": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^4.12.0",
    "eslint-config-universe": "^1.0.6",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^1.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "pkg": "^4.2.1",
    "prettier": "~1.7.4",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "@expo/bunyan": "^1.8.10",
    "@expo/json-file": "^5.2.0",
    "@expo/ngrok": "2.3.0",
    "@expo/simple-spinner": "^1.0.2",
    "babel-runtime": "^6.9.2",
    "chalk": "^2.0.1",
    "cli-table": "^0.3.1",
    "commander": "^2.9.0",
    "delay-async": "^1.0.0",
    "envinfo": "^3.11.1",
    "es6-error": "^3.0.0",
    "fs-extra": "^4.0.2",
    "glob": "^7.0.3",
    "indent-string": "^3.1.0",
    "inquirer": "^5.0.1",
    "lodash": "^4.17.4",
    "match-require": "^2.1.0",
    "mz": "^2.6.0",
    "ora": "^1.3.0",
    "progress": "^2.0.0",
    "qrcode-terminal": "^0.11.0",
    "semver": "^5.0.1",
    "slash": "^1.0.0",
    "source-map-support": "^0.4.1",
    "untildify": "^3.0.2",
    "xdl": "48.0.4"
  },
  "optionalDependencies": {
    "@expo/traveling-fastlane-darwin": "1.4.5",
    "@expo/traveling-fastlane-linux": "1.4.5"
  }
}
