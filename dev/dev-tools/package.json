{
  "name": "@expo/dev-tools",
  "version": "0.1.0-alpha.2",
  "main": "build/server/index.js",
  "scripts": {
    "dev": "NODE_ENV=development node ./scripts/dev-server.js",
    "build": "yarn clean && yarn build-server && yarn extract-fragment-types && yarn build-client",
    "build-client": "NODE_ENV=production next build && next export -o ./build/client",
    "build-server": "NODE_ENV=production babel ./server --ignore __tests__,__mocks__ --out-dir ./build/server",
    "extract-fragment-types": "node ./scripts/extract-fragment-types",
    "clean": "rm -rf ./build",
    "prepare": "yarn build",
    "start": "yarn dev",
    "test": "jest"
  },
  "files": [
    "build"
  ],
  "powertools": {
    "group": [
      "dev-tools",
      "exp"
    ]
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "copy-to-clipboard": "^3.0.8",
    "express": "4.16.3",
    "freeport-async": "1.1.1",
    "graphql": "0.13.2",
    "graphql-tools": "3.0.0",
    "iterall": "1.2.2",
    "lodash": "4.17.5",
    "subscriptions-transport-ws": "0.9.8"
  },
  "peerDependencies": {
    "xdl": "^50.0.0"
  },
  "devDependencies": {
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link-http": "^1.5.4",
    "apollo-link-ws": "^1.0.8",
    "apollo-server-express": "^1.3.6",
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-emotion": "^8.0.4",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "emotion": "^8.0.5",
    "emotion-server": "^8.0.5",
    "graphql-tag": "^2.9.1",
    "http-proxy-middleware": "^0.18.0",
    "jest": "^22.4.3",
    "next": "^5.1.0",
    "opn": "^5.3.0",
    "qrcode-react": "^0.1.16",
    "react": "16.2.0",
    "react-apollo": "^2.1.3",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "16.2.0",
    "react-emotion": "^8.0.5",
    "react-redux": "^5.0.7",
    "react-textarea-autosize": "^6.1.0",
    "redux": "^3.7.2",
    "slugify": "^1.0.2",
    "xdl": "^49.2.0"
  },
  "license": "MIT"
}
